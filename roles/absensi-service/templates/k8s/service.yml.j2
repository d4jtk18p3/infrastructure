---
apiVersion: v1
kind: Service
metadata:
  name: {{ service_frontend_name }}
  namespace: {{ service_namespace }}
spec:
  ports:
    - port: 80
      targetPort: 80
  selector:
    app: {{ service_frontend_name }}
---
apiVersion: v1
kind: Service
metadata:
  name: {{ service_absensi_name }}
  namespace: {{ service_namespace }}
spec:
  ports:
    - port: 80
      targetPort: 5001
  selector:
    app: {{ service_absensi_name }}